{
  "name": "mini-message-board",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "scss-watch": "sass --no-source-map --watch style.scss public/stylesheets/style.css",
    "nodemon-watch": "nodemon app.js -e *",
    "dev": "npm-run-all --parallel scss-watch nodemon-watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^7.0.2",
    "morgan": "~1.9.1",
    "sass": "^1.59.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5"
  }
}
